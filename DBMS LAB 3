CREATE TABLE DEPENDENT_13(
    ESSN Number(5),
    Name VARCHAR(10),
    Age Number(5),
    Relation VARCHAR(10),
    PRIMARY KEY(ESSN,Name)
);

ALTER TABLE DEPENDENT_13
      ADD FOREIGN KEY(ESSN) REFERENCES EMPLOYEE_13(SSN);
