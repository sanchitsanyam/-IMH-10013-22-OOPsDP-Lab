CREATE TABLE EMPLOYEE_13(
    NAME VARCHAR(25),
    SSN Number(5),
    BOD VARCHAR(10),
    ADDRESS VARCHAR(10),
    GENDER VARCHAR(1),
    salary Number(5),
    super_SSN  Number(5),
    DNO Number(5),
    PRIMARY KEY(SSN)
  
);
ALTER TABLE EMPLOYEE_13
      ADD FOREIGN KEY(super_SSN) REFERENCES  EMPLOYEE_13(SSN);
CREATE TABLE DEPARTMENT_13(
    DNO Number(5),
    DName VARCHAR(10),
    MGR_SSN Number(5),
    Start_Date VARCHAR(10),
    PRIMARY KEY(DNO)
);
ALTER TABLE EMPLOYEE_13
      ADD FOREIGN KEY(DNO) REFERENCES DEPARTMENT_13(DNO);
CREATE TABLE DEPARTMENT_location_13(
    DNO Number(5),
    DLocation VARCHAR(10),
    PRIMARY KEY(DNO)
);
ALTER TABLE DEPARTMENT_13
      ADD FOREIGN KEY(DNO) REFERENCES  DEPARTMENT_location_13(DNO);
